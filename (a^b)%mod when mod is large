ll binmultiply(ll a,ll b,ll mod)
{
     ll ans=0;
     while(b)
     {
          if(b&1)
          ans=(ans+a)%mod;
          
          a=(a+a)%mod;
          b>>=1;
     }
     return ans;
}

ll binwithmod(ll a,ll b,ll mod)
{
     a%=mod;
     ll ans=1;
     while(b)
     {
          if(b&1)
          ans=binmultiply(ans,a,mod);
          
          a=binmultiply(a,a,mod);
          b>>=1;
     }
     return ans;
}
